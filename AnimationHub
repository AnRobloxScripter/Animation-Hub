CustomGui.Parent = game:GetService("Players").LocalPlayer.PlayerGui
local Sound = Instance.new("Sound")
local Animation = Instance.new("Animation")
local function PlayAnimation(animid)
	local humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid
	Animation.AnimationId = "rbxassetid://"..animid
	humanoid:LoadAnimation(Animation):Play()
end
local function StopAllAnimations()
	local humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid
	for i, v in pairs(humanoid:GetPlayingAnimationTracks()) do
		v:Stop()
	end
end
local function PlayAudio(SoundID)
	local ws = game.Workspace
	Sound.SoundId = "rbxassetid://"..SoundID
	Sound.Parent = ws
	Sound.Looped = true
	Sound:Play()
end
local function StopAudio()
	Sound:Stop()
end
PlayAnim.Activated:Connect(function()
	PlayAnimation(AnimID.Text)
end)
StopAnim.Activated:Connect(function()
	StopAllAnimations()
end)
PlaySound.Activated:Connect(function()
	PlayAudio(SoundText.Text)
end)
StopSound.Activated:Connect(function()
	StopAudio()
end)
