local CanvasStroke = Instance.new("UIStroke")
CanvasStroke.Name = "CanvasStroke"
CanvasStroke.Thickness = 5
CanvasStroke.Color = Color3.fromRGB(26, 26, 26)
CanvasStroke.Parent = AnimationPlayer

local function PlayAnimation(animid)
	local humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid
	local Animation = Instance.new("Animation")
	Animation.AnimationId = "rbxassetid://"..animid
	humanoid:LoadAnimation(Animation):Play()
end
local function StopAllAnimations()
	local humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid
	for i, v in pairs(humanoid:GetPlayingAnimationTracks()) do
		v:Stop()
	end
end
PlayAnim.Activated:Connect(function()
	PlayAnimation(AnimID.Text)
end)
StopAnim.Activated:Connect(function()
	StopAllAnimations()
end)
